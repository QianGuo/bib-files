@techreport{goldwasser_cryptographic_2015,
	title = {Cryptographic {Assumptions}: {A} {Position} {Paper}},
	shorttitle = {Cryptographic {Assumptions}},
	url = {https://eprint.iacr.org/2015/907},
	abstract = {The mission of theoretical cryptography is to define and construct provably secure cryptographic protocols and schemes. Without proofs of security, cryptographic constructs offer no guarantees whatsoever and no basis for evaluation and comparison. As most security proofs necessarily come in the form of a reduction between the security claim and an intractability assumption, such proofs are ultimately only as good as the assumptions they are based on. Thus, the complexity implications of every assumption we utilize should be of significant substance, and serve as the yard stick for the value of our proposals.



Lately, the field of cryptography has seen a sharp increase in the number of new assumptions that are often complex to define and difficult to interpret. At times, these assumptions are hard to untangle from the constructions which utilize them.

We believe that the lack of standards of what is accepted as a reasonable cryptographic assumption can be harmful to the credibility of our field. Therefore, there is a great need for \{{\textbackslash}em measures\} according to which we classify and compare assumptions, as to which are \{{\textbackslash}it safe\} and which are not. In this paper, we propose such a classification and review recently suggested assumptions in this light. This follows the footsteps of Naor (Crypto 2003).

Our governing principle is relying on hardness assumptions that are independent of the cryptographic constructions.},
	number = {907},
	urldate = {2016-08-02},
	author = {Goldwasser, Shafi and Kalai, Yael Tauman},
	year = {2015},
	keywords = {assumptions},
	file = {ePrint IACR Full Text PDF:/home/qian/.zotero/zotero/x9qog5uo.default/zotero/storage/4SR4HCHT/Goldwasser and Kalai - 2015 - Cryptographic Assumptions A Position Paper.pdf:application/pdf;ePrint IACR Snapshot:/home/qian/.zotero/zotero/x9qog5uo.default/zotero/storage/QDFKHA66/907.html:text/html}
}

