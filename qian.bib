@incollection{heyse_lapin:_2012,
	series = {Lecture {Notes} in {Computer} {Science}},
	title = {Lapin: {An} {Efficient} {Authentication} {Protocol} {Based} on {Ring}-{LPN}},
	copyright = {©2012 Springer-Verlag Berlin Heidelberg},
	isbn = {978-3-642-34046-8 978-3-642-34047-5},
	shorttitle = {Lapin},
	abstract = {We propose a new authentication protocol that is provably secure based on a ring 
variant of the learning parity with noise (LPN) problem. The protocol follows the design principle 
of the LPN-based protocol from Eurocrypt’11 (Kiltz et al.), and like it, is a two round protocol 
secure against active attacks. Moreover, our protocol has small communication complexity and a very 
small footprint which makes it applicable in scenarios that involve low-cost, resource-constrained 
devices. Performance-wise, our protocol is more efficient than previous LPN-based schemes, such as 
the many variants of the Hopper-Blum (HB) protocol and the aforementioned protocol from 
Eurocrypt’11. Our implementation results show that it is even comparable to the standard 
challenge-and-response protocols based on the AES block-cipher. Our basic protocol is roughly 20 
times slower than AES, but with the advantage of having 10 times smaller code size. Furthermore, if 
a few hundred bytes of non-volatile memory are available to allow the storage of some off-line 
pre-computations, then the online phase of our protocols is only twice as slow as AES.},
	language = {en},
